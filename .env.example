# Copy to .env and fill in. Do not commit .env.

# Dropbox (get from https://www.dropbox.com/developers/apps)
# Leave REFRESH_TOKEN empty on first run to trigger OAuth in browser
DROPBOX_APP_KEY=
DROPBOX_APP_SECRET=
DROPBOX_REFRESH_TOKEN=

# LLM parser (fallback when XML/HTML parsing fails)
# LLM_PARSER_MODEL=openai | gemini (default: openai)
LLM_PARSER_MODEL=gemini
OPENAI_API_KEY=

# Gemini (when LLM_PARSER_MODEL=gemini)
GEMINI_API_KEY=
GEMINI_MODEL=gemini-2.5-flash-lite